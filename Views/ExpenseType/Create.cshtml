@model ExpenseType


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title text-center">Project Information Form</h4>
            <form method="post" class="forms-sample">
                <div class="form-group">
                    <label asp-for="Name" class="p-0"></label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
                
                <button type="submit" class="btn btn-primary"><i class="fas fa-plus"></i> Add</button>
                <a class="btn btn-primary btn-light" asp-action="Index" asp-controller="ExpenseType"><i class="fas fa-arrow-left"></i> Back To List</a>

            </form>
        </div>
    </div>
</div>
